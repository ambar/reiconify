{
  "name": "md.icons",
  "version": "0.0.1",
  "description": "Material icons for React",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "files": [
    "src",
    "es",
    "cjs"
  ],
  "scripts": {
    "start": "node ../reiconify/bin/reiconify --serve",
    "copy": "mkdir -p icons && rm -rf icons/** && node ./scripts/copyIcons",
    "clean": "rm -rf src/** es/** cjs/**",
    "build:static": "npm run build:src && rm -rf static/** && NODE_ENV=production node ../reiconify/bin/reiconify --static",
    "build:src": "rm -rf src/** && node ../reiconify/bin/reiconify --src icons/*.svg",
    "build": "npm run clean && npm run copy && node ../reiconify/bin/reiconify --src --es --cjs icons/*.svg",
    "prepare": "npm run build",
    "test": "echo 'ok'"
  },
  "license": "MIT",
  "devDependencies": {
    "globby": "^6.1.0",
    "material-design-icons": "^3.0.1"
  }
}
